    "querystring": "^0.2.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-check-box": "^2.1.7",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-material-ui": "^1.30.1",
    "react-native-navigation": "^4.4.0",
    "react-native-responsive-fontsize": "^0.4.3",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
